<map proto="1.5.0">
<!-- Created by ElBuenAnvita, built upon work by Cazadorian, _Pear and ElectroidFilms -->
<version>1.0.0</version>
<name>Tiki</name>
<objective>Destroy the enemy teams' egg and eliminate them before they get to yours.</objective>
<gamemode>bedwars</gamemode>
<game>EggWars</game>
<created>2026-01-09</created>
<respawn auto="true" delay="5s"/>
<time>50m</time>
<authors>
    <author contribution="Original map">MineLC Build Team</author> <!-- Unknown exact players :( -->
    <author uuid="a0959060-0a7c-40a1-8e3c-ceb2a4a31867" contribution="Layout, XML"/> <!-- ElBuenAnvita -->
</authors>
<blitz filter="blitz-conditions" join-filter="joining-filter" scoreboard-filter="blitz-conditions">
    <lives>1</lives>
    <broadcastLives>false</broadcastLives>
</blitz>
<kits>
    <kit id="default" reset-ender-pearls="true">
        <effect duration="4s" amplifier="5">damage_resistance</effect>
    </kit>
	<kit id="fireball" deduct-items="false">
        <item projectile="throwable-fireball" grenade="true" grenade-power="1.15" material="fireball" name="`rFireball"/>
    </kit>
</kits>
<actions>
	<action id="sudden-death" expose="true" filter="sudden-death-unstarted" scope="match">
        <set var="sudden_death_variable" value="1"/>
    </action>
	<trigger action="sudden-death" scope="match">
        <filter>
            <time>29m</time>
        </filter>
    </trigger>
	<trigger scope="match">
        <filter>
            <after message="`cSudden Death `rwill Begin In {0}" duration="60s" filter="sudden-death-counting"/>
        </filter>
        <action>
            <message id="sudden-death-title" fade-in="0.5s" stay="2s" fade-out="0.5s">
                <title>`cSUDDEN `cDEATH!</title>
                <subtitle>`fYou `fcan `fno `flonger `frespawn.</subtitle>
            </message>
            <set var="sudden_death_variable" value="2"/>
            <fill region="team-eggs" material="air" events="true"/>
        </action>
    </trigger>
	<message id="destroy-title" fade-in="0.5s" stay="2s" fade-out="0.5s">
        <title>`cEGG `cDESTROYED!</title>
        <subtitle>`fYou `fcan `fno `flonger `frespawn.</subtitle>
    </message>
	<trigger filter="egg-destroy-message" trigger="destroy-title" scope="player"/>
	<!-- sets -->
	<action id="upg-team-generator-iron-1" scope="player">
		<message text="El Generador Hierro #1 subió a Nivel 2"/>
		<set var="team_generator_iron_1" value="2" scope="player"/>
	</action>
	<action id="upg-team-generator-iron-2" scope="player">
		<message text="El Generador Hierro #1 subió a Nivel 3"/>
		<set var="team_generator_iron_1" value="3" scope="player"/>
	</action>
	<action id="upg-team-generator-iron-3" scope="player">
		<message text="El Generador Hierro #1 subió a Nivel 4"/>
		<set var="team_generator_iron_1" value="4" scope="player"/>
	</action>
</actions>
<variables>
	<variable id="sudden_death_variable" scope="match"/>
	<!-- Generators -->
	<!-- Team generators -->
	<variable id="team_generator_iron_1" scope="team" default="1"/>
	<variable id="team_generator_iron_2" scope="team" default="1"/>
	<variable id="team_generator_gold_1" scope="team" default="0"/>
	<variable id="team_generator_diamond_1" scope="team" default="0"/>
	<!-- Global (Middle) generators -->
	<variable id="global_generator" scope="match" default="1"/>
</variables>
